<header class="header">
  <div class="title">
    <a href="<%= url_for() %>" class="logo" target="_self">
      <%= config.title %>
    </a>
  </div>
  <nav class="navbar">
    <ul class="menu">
      <% for (name in theme.menu) { %>
        <li class="menu-item">
          <a href="<%- url_for(theme.menu[name]) %>" class="menu-item-link" target="_self"><%= name %></a>
        </li>
      <% } %> 
    </ul>
  </nav>
  <div></div>
</header>
