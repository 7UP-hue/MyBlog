.header {
  height: 60px;
  color: #333;
  justify-content: space-between;
  background-color: rgb(255,255,255);
  border-bottom: 1px solid #ccc;
  box-shadow: 5px 10px 20px #ccc;
  align-items: center;
  padding-left: 50px;
  padding-right: 50px;
  font-family: SimSun;
  position: fixed;
  top: 0;
  size: 0;
  width: 100%;
  z-index: 10;
  display: none;
}
a {
  text-decoration: none;
}
.menu-item-link,.logo {
  color: #666;
}
.menu-item-link:hover,.logo {
  color: #30AD91;
}
.menu {
  display: flex;
}
.menu li {
  margin-left: 10px;
  font-size: 16px;
}
li {
  list-style: none;
}
.header-last
  display: flex;